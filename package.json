{
  "name": "icoziv",
  "version": "1.0.0",
  "main": "index.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "wrangler dev",
    "build": "bun run build.ts",
    "deploy": "bun run build.ts && wrangler deploy",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "check": "bun run lint && bun run format && vitest",
    "check:fix": "bun run lint:fix && bun run format:fix && bun run test"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.45.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.42.0"
  }
}
